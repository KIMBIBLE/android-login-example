<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bbkimAssign">
    android:versionCode="3"
    android:versionName="3.0"

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_hdpi"
        android:label="sju_assignment"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.bbkimAssign.MainActivity"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.LoginRegister">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
            <intent-filter>

                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="bbkim"
                    android:scheme="http" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bbkimAssign.ProfileActivity"
            android:theme="@style/AppTheme.Profile" />
        <activity android:name="com.bbkimAssign.VideoActivity"></activity>
    </application>

</manifest>